{
    "name": "amethyst/price",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-price", "price"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*",
        "amethyst/price-rule": "0.2.*",
        "amethyst/consume-rule": "0.2.*",
        "amethyst/target": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "amethyst/foo": "0.2.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "price",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\PriceServiceProvider"
            ]
        }
    }
}
